{{!
  string? html-subtitle
  string html-undelete
  string html-bodycontent
  string html-printfooter
  string html-catlinks
  string html-data-after-content
}}

{{#data-content}}
<main id="content" class="mw-body mw-body-content">
  <div id="contentSub">{{#html-subtitle}}{{{.}}}{{/html-subtitle}}</div>
  {{#html-undelete}}{{{.}}}{{/html-undelete}}
  {{{html-bodycontent}}}
  {{! Outputs a css clear using the core visualClear class}}
  <div class="visualClear"></div>
  <div class="printfooter">
    {{{html-printfooter}}}
  </div>
  {{{html-catlinks}}}
  {{{html-data-after-content}}}
</main>
<hr id="content-end-bar" />
{{/data-content}}
