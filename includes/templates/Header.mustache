{{!
  string? html-sitenotice
  string? html-newtalk
  string html-namespaces
  string? html-watch
  string page-language
  string html-title
  string? html-title-buttons
  string html-toolbox
  string html-actions
  string? page-history
  string? page-lastmod
  string? html-views
}}

<header id="p-header">
  {{#html-sitenotice}}
  <div id="siteNotice">{{{html-sitenotice}}}</div>
  {{/html-sitenotice}}
  {{#html-newtalk}}<div class="usermessage">{{{html-newtalk}}}</div>{{/html-newtalk}}
  {{{html-namespaces}}}
  {{#html-watch}}{{{html-watch}}}{{/html-watch}}
  <div id="p-title-and-tb">
    <h1 class="firstHeading" lang="{{language}}">{{{html-title}}}</h1>
    {{{html-title-buttons}}}
    <div id="p-actions-and-toolbox">
      {{{html-toolbox}}}
      {{{html-actions}}}
    </div>
  </div>
  <div id="lastmod-and-views">
    {{#page-history}}
    <a id="lastmod" href="{{page-history}}">{{{page-lastmod}}}</a>
    {{/page-history}}
    {{{html-views}}}
  </div>
</header>
